{
  "name": "ecom-option-group-api",
  "version": "0.0.1",
  "description": "API for Option Group microservice Edit",
  "repository": "git@github.com:photobox/ecom-option-group-api.git",
  "author": "Photobox",
  "scripts": {
    "build:ui": "webpack",
    "dev:ui": "webpack-dev-server --hot --inline --content-base build/",
    "dev": "nodemon src/index.js",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "prettier": "prettier 'src/**/*.js' --write --single-quote=true --print-width=120 --trailing-comma=es5",
    "test": "jest --testRegex='\\.test\\.js$'",
    "test:integration": "jest --testRegex='\\.test\\.int\\.js$'",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch"
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^20.0.1",
    "eslint-plugin-wyze": "^3.2.1",
    "husky": "^0.13.3",
    "jest": "^20.0.1",
    "lint-staged": "^3.4.1",
    "nodemon": "^1.11.0",
    "prettier": "^1.3.1",
    "supertest": "^3.0.0",
    "webpack": "^2.5.1"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "babel-plugin-transform-es2015-modules-commonjs",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "ajv": "^5.1.1",
    "aws-sdk": "^2.50.0",
    "babel-preset-react": "^6.24.1",
    "data.either": "^1.5.0",
    "koa": "^2.2.0",
    "koa-basic-auth": "^2.0.0",
    "koa-bodyparser": "^4.2.0",
    "koa-cors": "^0.0.16",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-jsonschema-form": "^0.48.2",
    "uuid": "^3.0.1"
  }
}
