{
  "name": "@photobox/cruddy-simple",
  "version": "0.1.0",
  "description": "Node CRUD API",
  "author": "Photobox",
  "contributors": [
    "Matt Phillips <matt.phillips@photobox.com>"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "scripts": {
    "build": "babel src -d lib  --ignore *.test*.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint src",
    "prepublish": "yarn build",
    "precommit": "lint-staged",
    "prettier": "prettier 'src/**/*.js' --write --single-quote=true --print-width=120 --trailing-comma=es5",
    "start": "node lib/index.js",
    "test": "jest --testRegex='\\.test\\.js$'",
    "test:integration": "jest --testRegex='\\.test\\.int\\.js$'",
    "test:coverage": "yarn test -- --coverage",
    "test:ci:coverage": "yarn test -- --coverage --runInBand",
    "test:report": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test:watch": "yarn test -- --watch"
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^20.0.1",
    "eslint-plugin-wyze": "^3.2.1",
    "husky": "^0.13.3",
    "jest": "^20.0.1",
    "jest-each": "^0.2.0",
    "lint-staged": "^3.4.1",
    "nodemon": "^1.11.0",
    "prettier": "^1.3.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "plugins": [
      "babel-plugin-transform-es2015-modules-commonjs",
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ]
  },
  "dependencies": {
    "ajv": "^5.1.1",
    "aws-sdk": "^2.50.0",
    "coveralls": "^2.13.1",
    "data.either": "^1.5.0",
    "koa": "^2.2.0",
    "koa-basic-auth": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-conditional-get": "^2.0.0",
    "koa-cors": "^0.0.16",
    "koa-etag": "^3.0.0",
    "koa-router": "^7.1.1",
    "ramda": "^0.23.0",
    "swagger2": "^0.0.24",
    "swagger2-koa": "^0.0.36",
    "uuid": "^3.0.1"
  }
}
