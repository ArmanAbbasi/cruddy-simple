{
  "name": "ecom-option-group-api",
  "version": "0.0.1",
  "description": "API for Option Group microservice Edit",
  "repository": "git@github.com:photobox/ecom-option-group-api.git",
  "author": "Photobox",
  "scripts": {
    "dev": "nodemon | babel-node src/index.js",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "prettier": "prettier 'src/**/*.js' --write --single-quote=true --print-width=120 --trailing-comma=es5",
    "test": "jest",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch"
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^20.0.1",
    "eslint-plugin-wyze": "^3.2.1",
    "husky": "^0.13.3",
    "jest": "^20.0.1",
    "lint-staged": "^3.4.1",
    "nodemon": "^1.11.0",
    "prettier": "^1.3.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.50.0",
    "koa": "^2.2.0",
    "koa-basic-auth": "^2.0.0",
    "koa-bodyparser": "^4.2.0",
    "koa-cors": "^0.0.16",
    "uuid": "^3.0.1"
  }
}
